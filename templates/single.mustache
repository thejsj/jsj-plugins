	<div class="row">
		<div class="large-10 large-offset-1 columns">
			<div class="page-title {{# post.sub_title }}has-paragraph{{/ post.sub_title }} {{# post.featured_content }}has-featured-content{{/ post.featured_content }}">
				<h2>{{ post.post_title }}</h2>
				{{# post.sub_title }}
				<p>{{ post.sub_title }}</p>
				{{/ post.sub_title }}
			</div>
		</div>
	</div>
	{{# post.featured_content }}
	<div class="row featured-image">
		<div class="large-10 large-centered columns">
			{{& post.featured_content }}
		</div>
	</div>
	{{/ post.featured_content }}
	<div>
		<div class="row">
			<div class="large-3 columns link-list">
			<ul>
			{{# post.links }}
				<li><a href="{{& url }}" target="{{ target }}" class="button large">{{ title }}</a></li>
			{{/ post.links }}
			</ul>
			</div>
			<div class="large-8 columns end"><!-- large-centered -->
				<div class="entry-content">
					{{& post.post_content }}
				</div>
